{
  "name": "@semanticweb/faker",
  "version": "0.1.0",
  "main": "dist/index",
  "types": "dist/index",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "yarn run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "predocs": "npm run compile",
    "docs": "typedoc src",
    "docs:watch": "chokidar \"**/*.ts\" -c \"yarn run docs\"",
    "docs:serve": "serve docs",
    "docs:dev": "concurrently \"npm run docs:watch\" \"npm run docs:serve\""
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.1.0",
    "jest": "^25.3.0",
    "serve": "^11.3.0",
    "ts-jest": "^25.3.1",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/rdf-ext": "^1.3.6",
    "@tpluscode/rdf-ns-builders": "^0.1.0",
    "@types/faker": "^4.1.11",
    "@types/jsonld": "^1.5.1",
    "@types/rdf-js": "^2.0.12",
    "@zazuko/rdf-vocabularies": "^2020.3.23",
    "faker": "^4.1.0",
    "jsonld": "^3.0.1",
    "rdf-ext": "^1.3.0"
  }
}